            { "href": "/accounts/jsmith@mail.com/tasks?page=3", "rel":  "next /rels/tasks-collection", "description": "Next page" },
            { "href": "/accounts/jsmith@mail.com/tasks?page=25", "rel":  "last /rels/tasks-collection", "description": "Last page" },
